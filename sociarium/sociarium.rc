/////////////////////////////////////////////////////////////////////////////
	POPUP "描画(&V)"
	BEGIN
		MENUITEM "ノード(&1)\t1",				IDM_SHOW_NODE
		MENUITEM "エッジ(&2)\t2",				IDM_SHOW_EDGE
		MENUITEM "コミュニティ(&3)\t3",			IDM_SHOW_COMMUNITY
		MENUITEM "コミュニティエッジ(&4)\t4",	IDM_SHOW_COMMUNITY_EDGE
		MENUITEM SEPARATOR
		MENUITEM "ノードの名前(&5)\tShift+Ctrl+1",				IDM_SHOW_NODE_LABEL
		MENUITEM "エッジの名前(&6)\tShift+Ctrl+2",				IDM_SHOW_EDGE_LABEL
		MENUITEM "コミュニティの名前(&5)\tShift+Ctrl+3",		IDM_SHOW_COMMUNITY_LABEL
		MENUITEM "コミュニティエッジの名前(&6)\tShift+Ctrl+4",	IDM_SHOW_COMMUNITY_EDGE_LABEL
		MENUITEM SEPARATOR
		POPUP	"ノードの描画方法(&7)\tCtrl+1"
		BEGIN
			MENUITEM "ポリゴン(&1)",	IDM_NODE_STYLE_POLYGON
			MENUITEM "テクスチャ(&2)",	IDM_NODE_STYLE_TEXTURE
		END
		POPUP	"エッジの描画方法(&8)\tCtrl+2"
		BEGIN
// 			MENUITEM "直線(&1)",	IDM_EDGE_STYLE_STRAIGHT
// 			MENUITEM "2D曲線(&2)",	IDM_EDGE_STYLE_HCURVE
// 			MENUITEM "3D曲線(&3)",	IDM_EDGE_STYLE_VCURVE
// 			MENUITEM SEPARATOR
// 			MENUITEM "ライン(&4)",	IDM_EDGE_STYLE_LINE
// 			MENUITEM "テクスチャ(&5)",	IDM_EDGE_STYLE_TEXTURE
 			MENUITEM "ライン(&1)",	IDM_EDGE_STYLE_LINE
 			MENUITEM "テクスチャ(&2)",	IDM_EDGE_STYLE_TEXTURE
		END
		POPUP	"コミュニティの描画方法(&8)\tCtrl+3"
		BEGIN
			MENUITEM "ポリゴン(&1)",	IDM_COMMUNITY_STYLE_POLYGON_CIRCLE
			MENUITEM "テクスチャ(&2)",	IDM_COMMUNITY_STYLE_TEXTURE
		END
		POPUP	"コミュニティエッジの描画方法(&9)\tCtrl+4"
		BEGIN
//			MENUITEM "直線(&1)",	IDM_COMMUNITY_EDGE_STYLE_STRAIGHT
//			MENUITEM "2D曲線(&2)",	IDM_COMMUNITY_EDGE_STYLE_HCURVE
//			MENUITEM "3D曲線(&3)",	IDM_COMMUNITY_EDGE_STYLE_VCURVE
//			MENUITEM SEPARATOR
//			MENUITEM "ライン(&4)",	IDM_COMMUNITY_EDGE_STYLE_LINE
//			MENUITEM "テクスチャ(&5)",	IDM_COMMUNITY_EDGE_STYLE_TEXTURE
			MENUITEM "ライン(&1)",	IDM_COMMUNITY_EDGE_STYLE_LINE
			MENUITEM "テクスチャ(&2)",	IDM_COMMUNITY_EDGE_STYLE_TEXTURE
		END
		MENUITEM SEPARATOR
		POPUP "フォントのサイズ(&F)"
		BEGIN
			POPUP "ノードの名前(&1)"
			BEGIN
				MENUITEM	"小(&1)"	IDM_FONT_SIZE_NODE_0
				MENUITEM	"標準(&2)"	IDM_FONT_SIZE_NODE_1
				MENUITEM	"大(&3)"	IDM_FONT_SIZE_NODE_2
				MENUITEM	"特大(&4)"	IDM_FONT_SIZE_NODE_3
			END
			POPUP "エッジの名前(&2)"
			BEGIN
				MENUITEM	"小(&1)"	IDM_FONT_SIZE_EDGE_0
				MENUITEM	"標準(&2)"	IDM_FONT_SIZE_EDGE_1
				MENUITEM	"大(&3)"	IDM_FONT_SIZE_EDGE_2
				MENUITEM	"特大(&4)"	IDM_FONT_SIZE_EDGE_3
			END
			POPUP "コミュニティの名前(&3)"
			BEGIN
				MENUITEM	"小(&1)"	IDM_FONT_SIZE_COMMUNITY_0
				MENUITEM	"標準(&2)"	IDM_FONT_SIZE_COMMUNITY_1
				MENUITEM	"大(&3)"	IDM_FONT_SIZE_COMMUNITY_2
				MENUITEM	"特大(&4)"	IDM_FONT_SIZE_COMMUNITY_3
			END
			POPUP "コミュニティエッジの名前(&4)"
			BEGIN
				MENUITEM	"小(&1)"	IDM_FONT_SIZE_COMMUNITY_EDGE_0
				MENUITEM	"標準(&2)"	IDM_FONT_SIZE_COMMUNITY_EDGE_1
				MENUITEM	"大(&3)"	IDM_FONT_SIZE_COMMUNITY_EDGE_2
				MENUITEM	"特大(&4)"	IDM_FONT_SIZE_COMMUNITY_EDGE_3
			END
		END
		MENUITEM SEPARATOR
		POPUP	"ノードのサイズ(&N)"
		BEGIN
			MENUITEM "均等(&1)",			IDM_NODE_SIZE_UNIFORM
			MENUITEM "次数中心性(&2)",		IDM_NODE_SIZE_REPRESENTS_DEGREE_CENTRALITY
			MENUITEM "距離中心性(&3)",		IDM_NODE_SIZE_REPRESENTS_CLOSENESS_CENTRALITY
			MENUITEM "媒介中心性(&4)",		IDM_NODE_SIZE_REPRESENTS_BETWEENNESS_CENTRALITY
			MENUITEM "ページランク(&5)",	IDM_NODE_SIZE_REPRESENTS_PAGERANK
			MENUITEM SEPARATOR
			MENUITEM "点(&0)",				IDM_NODE_SIZE_NONE
		END
		MENUITEM SEPARATOR
		MENUITEM "レイヤー切り替えスライダー(&S)\tS",	IDM_SHOW_SLIDER
		MENUITEM "レイヤーの名前(&T)\tT",				IDM_SHOW_TIME_LABEL
		MENUITEM "グリッド(&G)\tG",						IDM_SHOW_GRID
		MENUITEM "中心点/注視点(&C)",					IDM_SHOW_CENTER
		MENUITEM "FPS(&P)",								IDM_SHOW_FPS
		MENUITEM SEPARATOR
		MENUITEM "視点の初期化(&V)\tHome",	IDM_KEY_HOME
		MENUITEM "拡大(&Z)\tPage Down",		IDM_KEY_SHIFT_CTRL_X
		MENUITEM "縮小(&X)\tPage Up",		IDM_KEY_SHIFT_CTRL_Z
	END

